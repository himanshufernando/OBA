<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="@color/myWhite">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="80dip">

        <RelativeLayout
            android:layout_width="80dip"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_margin="5dip"
            android:id="@+id/relativeLayou20"
            android:background="@drawable/pdf">

        </RelativeLayout>

        <tkhub.project.mscoba.MyClass.Font.FontHedar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Medium Text"
            android:id="@+id/TextView_NewsLetters_Title"
            android:layout_marginLeft="5dip"

            android:textColor="@color/colorPrimary"
            android:layout_toRightOf="@+id/relativeLayou20"
            android:layout_toEndOf="@+id/relativeLayou20"
            android:layout_centerVertical="true" />

    </RelativeLayout>
</LinearLayout>
