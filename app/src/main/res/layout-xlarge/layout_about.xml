<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:theme="@style/AppTheme">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#ffffff">

        <RelativeLayout
            android:id="@+id/relativeLayout_heder"
            android:layout_width="match_parent"
            android:layout_height="60dip"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:visibility="visible"
            android:background="@color/myBlue">

            <tkhub.project.mscoba.MyClass.Font.FontHedar
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="ABOUT US"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="#ffffff" />

            <RelativeLayout
                android:id="@+id/layout_navigation"
                android:layout_width="60dip"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true">


                <com.balysv.materialmenu.MaterialMenuView
                    android:id="@+id/action_bar_menu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    app:mm_color="@android:color/white" />


            </RelativeLayout>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="70dip"
            android:layout_alignParentBottom="true"
            android:background="@color/myBlue"
            android:id="@+id/relativeLayout23">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:id="@+id/relativeLayout_MSC"
                android:layout_marginLeft="40dip">

                <tkhub.project.mscoba.MyClass.Font.TextViewFontAwesome
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/icon_msc"
                    android:id="@+id/textView28"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:textColor="#ffffff"
                    android:textSize="40sp"
                    android:layout_marginTop="2dip" />

                <tkhub.project.mscoba.MyClass.Font.FontHedar
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="MSC"
                    android:id="@+id/textView29"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:textColor="#ffffff"
                    android:textAppearance="?android:attr/textAppearanceMedium" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="200dip"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:id="@+id/relativeLayout21"></RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_toLeftOf="@+id/relativeLayout21"
                android:layout_toStartOf="@+id/relativeLayout21"
                android:id="@+id/relativeLayout_feedback">

                <tkhub.project.mscoba.MyClass.Font.TextViewFontAwesome
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/icon_hisory"
                    android:id="@+id/textView30"
                    android:textColor="@color/myWhite"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="2dip"
                    android:textSize="40sp" />

                <tkhub.project.mscoba.MyClass.Font.FontHedar
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="History"
                    android:id="@+id/textView31"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:textColor="#ffffff"
                    android:textAppearance="?android:attr/textAppearanceMedium" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="50dip"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/relativeLayout21"
                android:layout_toEndOf="@+id/relativeLayout21"
                android:id="@+id/relativeLayout_like">

                <tkhub.project.mscoba.MyClass.Font.TextViewFontAwesome
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/icon_building"
                    android:id="@+id/textView32"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="3dip"
                    android:textColor="@color/myWhite"
                    android:textSize="40sp" />

                <tkhub.project.mscoba.MyClass.Font.FontHedar
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="OBA"
                    android:id="@+id/textView33"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:textColor="@color/myWhite"
                    android:textAppearance="?android:attr/textAppearanceMedium" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="40dip"
                android:id="@+id/relativeLayout_developer">

                <tkhub.project.mscoba.MyClass.Font.TextViewFontAwesome
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/icon_developer"
                    android:id="@+id/textView34"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="4dip"
                    android:textColor="@color/myWhite"
                    android:textSize="40sp" />

                <tkhub.project.mscoba.MyClass.Font.FontHedar
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Developer"
                    android:id="@+id/textView35"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:textColor="@color/myWhite"
                    android:textAppearance="?android:attr/textAppearanceMedium" />
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/relativeLayout_heder"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_above="@+id/relativeLayout23"
            android:id="@+id/relativeLayout_about_main_msc"
            android:background="#ffffff">

            <ScrollView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/scrollView2"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:background="#ffffff">

                <LinearLayout

                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:orientation="vertical"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/relativeLayout2"
                    android:background="#FFFFFF">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:background="#ffffff">

                        <com.mikhaellopez.circularimageview.CircularImageView
                            android:layout_width="400dip"
                            android:layout_height="400dip"
                            app:civ_border_color="@color/myBlue"
                            app:civ_border_width="4dp"
                            app:civ_shadow="true"
                            android:id="@+id/imageView6"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="20dip"
                            android:src="@drawable/aboutone" />

                        <tkhub.project.mscoba.MyClass.Font.FontHedar
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="@string/string_about_one"
                            android:textColor="@color/mytextblack"
                            android:id="@+id/textView38"
                            android:layout_below="@+id/imageView6"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:layout_marginTop="5dip"
                            android:layout_marginBottom="10dip"
                            android:layout_marginLeft="5dip" />


                    </RelativeLayout>


                </LinearLayout>

            </ScrollView>


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/realtivelayout_about_main_history"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/relativeLayout_heder"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_above="@+id/relativeLayout23"
            android:background="#ffffff">
            <ScrollView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/scrollView3"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:background="#ffffff">

                <LinearLayout

                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:orientation="vertical"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/relativeLayout2"
                    android:background="#FFFFFF">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:background="#ffffff">

                        <com.mikhaellopez.circularimageview.CircularImageView
                            android:layout_width="400dip"
                            android:layout_height="400dip"
                            app:civ_border_color="@color/myBlue"
                            app:civ_border_width="4dp"
                            app:civ_shadow="true"
                            android:id="@+id/imageView7"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="20dip"
                            android:src="@drawable/abouttwo" />

                        <tkhub.project.mscoba.MyClass.Font.FontHedar
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="@string/string_about_two"
                            android:textColor="@color/mytextblack"
                            android:id="@+id/textView39"
                            android:layout_below="@+id/imageView7"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:layout_marginTop="5dip"
                            android:layout_marginBottom="10dip"
                            android:layout_marginLeft="5dip" />


                    </RelativeLayout>


                </LinearLayout>

            </ScrollView>


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/realtivelayout_about_main_oba"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/relativeLayout_heder"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_above="@+id/relativeLayout23"
            android:background="#ffffff">
            <ScrollView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/scrollView4"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:background="#ffffff">

                <LinearLayout

                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:orientation="vertical"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/relativeLayout2"
                    android:background="#FFFFFF">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:background="#ffffff">

                        <com.mikhaellopez.circularimageview.CircularImageView
                            android:layout_width="400dip"
                            android:layout_height="400dip"
                            app:civ_border_color="@color/myBlue"
                            app:civ_border_width="4dp"
                            app:civ_shadow="true"
                            android:id="@+id/imageView8"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="40dip"
                            android:src="@drawable/memberslo" />

                        <tkhub.project.mscoba.MyClass.Font.FontHedar
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="@string/string_about_three"
                            android:textColor="@color/mytextblack"
                            android:id="@+id/textView40"
                            android:layout_below="@+id/imageView8"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:layout_marginTop="5dip"
                            android:layout_marginBottom="10dip"
                            android:layout_marginLeft="5dip" />


                    </RelativeLayout>


                </LinearLayout>

            </ScrollView>


        </RelativeLayout>


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/drawer"
        android:layout_width="400dp"
        android:layout_height="match_parent"
        android:layout_below="@+id/Relative_TitleBar"
        android:layout_gravity="start"
        android:background="#cc3F51B5"
        android:clickable="false">



        <ListView
            android:id="@+id/listView_navigation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:divider="@color/myBlue"
            android:layout_marginTop="60dp" />
    </RelativeLayout>


</android.support.v4.widget.DrawerLayout>

